{
  "name": "scully-plugins",
  "version": "0.0.0",
  "workspaces": [
    "projects/ngx-translate-router-scully-plugin"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && npm run scully:scan",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test.watch": "ng test @0nza1101/ngx-translate-router-scully-plugin",
    "test.ci": "ng test @0nza1101/ngx-translate-router-scully-plugin --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "build.lib": "tsup",
    "scully:scan": "npx scully --scanRoutes --noPrompt --showGuessError",
    "scully:serve": "npx scully serve --",
    "build.all": "npm run build.lib && npm run build",
    "publish": "npm run package && npm publish ./dist/ngx-translate-router-scully-plugin --access public",
    "package": "npm run build.lib && shx cp ./projects/ngx-translate-router-scully-plugin/package.json ./dist/ngx-translate-router-scully-plugin && shx cp README.md ./dist/ngx-translate-router-scully-plugin"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.3",
    "@angular/common": "^15.2.3",
    "@angular/compiler": "^15.2.3",
    "@angular/core": "^15.2.3",
    "@angular/forms": "^15.2.3",
    "@angular/platform-browser": "^15.2.3",
    "@angular/platform-browser-dynamic": "^15.2.3",
    "@angular/router": "^15.2.3",
    "@gilsdav/ngx-translate-router": "^6.1.0",
    "@gilsdav/ngx-translate-router-http-loader": "^2.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@scullyio/init": "^2.1.41",
    "@scullyio/ng-lib": "^2.1.41",
    "@scullyio/scully": "^2.1.41",
    "@scullyio/scully-plugin-puppeteer": "^2.1.41",
    "rxjs": "^7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "^0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular/cli": "^15.2.4",
    "@angular/compiler-cli": "^15.2.3",
    "@angular/language-service": "^15.2.3",
    "@types/jasmine": "~4.3.1",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.15.3",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.2.2",
    "shx": "^0.3.4",
    "tslint": "~6.1.3",
    "tsup": "^6.7.0",
    "typescript": "~4.9.3"
  }
}
